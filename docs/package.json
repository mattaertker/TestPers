{
  "name": "digitalwebflex",
  "version": "1.0.0",
  "description": "landing page for digital web flex",
  "main": "index.js",
  "private": true,
  "proxy": "http://127.0.0.1:3001",
  "dependencies": {
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.4.1",
    "cross-env": "^7.0.1",
    "jquery": "^3.4.1",
    "live-server": "^1.2.1",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.12.0",
    "react-google-recaptcha": "^2.0.1",
    "react-image": "^2.3.0",
    "react-recaptcha": "^2.3.10",
    "react-recaptcha-google": "^1.1.1",
    "reactstrap": "^8.4.1",
    "reaptcha": "^1.7.2",
    "recaptcha": "^1.2.1",
    "simplify": "^1.0.0",
    "simplifyify": "^7.0.7",
    "three": "^0.110.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "autoprefixer": "^9.7.4",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "compression-webpack-plugin": "^3.1.0",
    "concat": "^1.0.3",
    "file-loader": "^5.0.2",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "scripts": {
    "bundle": "cross-env webpack --config ./webpack.prod.config.js --progress --colors && cp index.html dist",
    "watch:sass": "node-sass SASS/main.scss css/style.css -w",
    "devserver": "live-server",
    "build": "webpack --config webpack.config.js -w",
    "start": "npm-run-all --parallel devserver watch:sass build",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "bundle:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "'"
  },
